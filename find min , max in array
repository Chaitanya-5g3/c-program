#include <stdio.h>
int main()
{
  int a[10], i, n, sum=0,min, max;
  printf("enter the size");
  scanf("%d",&n);
  printf("enter the array elements");
  for(i=0;i<n;i++)
  {
    scanf("%d",&a[i]);
  }
  max=a[0], min=a[0];
  for(i=0;i<n;i++)
  {
    if(a[i]>max)
    {
      max=a[i];
    }
  
   if(a[i]<min)
   {
     min=a[i];
   }
 
  }
  printf("max is%d",max);
  printf("min is%d",min);
}